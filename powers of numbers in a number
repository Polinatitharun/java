package new1;
import java.util.*;
public class new1 {

	public static void main(String[] args) {
		int n,r,d=1,p,s=0;
		Scanner sc=new Scanner(System.in);
		n=321;
		while(n>0) {
			r=n%10;
			p=1;
			for(int i=1;i<=d;i++) {
				p=p*r;
			}
			s=s+p;
			d++;
			n=n/10;
		}
		System.out.println(s);

	}

}
